{
  "name": "Laid-back-theme",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#C9C3BD",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8D9199B8"
      }
    },
    {
      "name": "Base types",
      "scope": [
        "storage.type.js",
        "keyword.type.cs",
        "entity.name.type.class.cs",
        "storage.type.cs",
        "entity.name.type.interface.cs",
        "punctuation.definition.typeparameters",
        "meta.brace.square.js",
        "string.quoted.template.js",
        "punctuation.separator.question-mark.cs",
        "entity.name.tag.inline.any.html",
        "keyword.other.var.cs",
        "entity.name.type.type-parameter.cs"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#49bce9ca"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace.curly.litobj.js",
        "meta.brace.curly.js",
        "variable.language.this"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#797c9bca"
      }
    },
    {
      "name": "Syntax",
      "scope": [
        "storage.modifier",
        "keyword.other.await.cs",
        "keyword.other.using.cs"
      ],
      "settings": {
        "foreground": "#797c9bca"
      }
    },
    {
      "name": "Gets and sets",
      "scope": [
        "keyword.other.get.cs",
        "keyword.other.set.cs"
      ],
      "settings": {
        "foreground": "#797c9bca"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "keyword.other.class.cs",
        "keyword.other.interface.cs",
        "keyword.other.base.cs",
        "keyword.other.namespace.cs",
        "entity.name.tag.script.html",
        "entity.name.tag.template.html",
        "entity.name.tag.style.html",
        "keyword.other.new.cs"
      ],
      "settings": {
        "foreground": "#f1c778ca"
      }
    },
    {
      "name": "Methods",
      "scope": [
        "keyword.other.typeof.cs",
        "entity.name.function",
        "punctuation.parenthesis",
        "punctuation.definition.parameters.end",
        "punctuation.definition.parameters.begin",
        "meta.brace.round.js",
        "keyword.other.nameof.cs"
      ],
      "settings": {
        "foreground": "#2dc272"
      }
    },
    {
      "name": "type parameters",
      "scope": [],
      "settings": {
        "foreground": "#ffffffca"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.control.conditional.js",
        "keyword.control.flow.return.cs",
        "keyword.other.await.cs",
        "keyword.control",
        "keyword.operator.null-coalescing.cs"
      ],
      "settings": {
        "foreground": "#f0697bca"
      }
    },
    {
      "name": "namespaces",
      "scope": [
        "entity.name.type.namespace.cs"
      ],
      "settings": {
        "foreground": "#8D9199B8"
      }
    },
    {
      "name": "string",
      "scope": [
        "string.quoted.double.cs"
      ],
      "settings": {
        "foreground": "#9ab4ed"
      }
    }
  ]
}